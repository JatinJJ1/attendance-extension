{
    "manifest_version": 2,
  
    "name": "Attendance Manager",
    "description": "Quick launch attendance taker",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
  }